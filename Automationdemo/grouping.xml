<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<test name="Test"></test>
	<parameter name="browser" value="edge" />
	<test name="groupingtest">
		<groups>
			<run>
				<include name="SanityTest" />
			</run>
		</groups>
		<classes>
			<class name="LoginTestcase.InvalidpassLogin"></class>
			<class name="LoginTestcase.ValidCredential"></class>
		</classes>
	</test>
</suite>